<%- include('partials/header') -%>
<div class="container py-5">
  <h1 class="mb-4">Your Weekly Dinner Plan 🗓️</h1>

  <% dinners.forEach(dinner => { %>
    <div class="card mb-4 shadow-sm">
      <div class="card-header bg-info text-white"><%= dinner.title %></div>
      <div class="card-body">
        <pre><%= dinner.content %></pre>
      </div>
    </div>
  <% }) %>

  <a href="/" class="btn btn-outline-primary mt-4">Plan Another Dinner</a>
</div>
<%- include('partials/footer') -%>